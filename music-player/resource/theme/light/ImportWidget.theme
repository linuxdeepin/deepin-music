#ImportWidget {
    background-color: rgba(255,255,255,1);
    border-radius: 4px;
}

#ImportViewLogo {
    image: url(:/common/image/deepin-music.svg);
}

#ImportViewText{
    font-size: 12px;
    line-height: 1.5;
    text-align: center;
    color: #979797;
}

#ImportViewImportButton {
    font-family: SourceHanSansSC-Medium;
    font-size: 14px;
    color: #0599FF;
    border-image: url(:/light/image/suggest_button_normal.png);
}
#ImportViewImportButton:hover {
    color: white;
    border-image: url(:/light/image/suggest_button_hover.png);
}
#ImportViewImportButton:pressed {
    color: #2ca7f8;
    border-image: url(:/light/image/suggest_button_press.png);
}
